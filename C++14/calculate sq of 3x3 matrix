#include <iostream.h>
#include <conio.h>
//calculate square of 3x3 matrix

int *sq(int *a[3][3])
{
    static int b[3][3]; 
    int i, j;
    
   
    for(i=0 ; i<3 ; i++)
    {
        for(j=0 ; j<3 ; j++)
        {
            b[i][j]= a[i][0]*a[0][j] + a[i][1]*a[1][j] + a[i][2]*a[2][j];
        }
    }
}
